{
    "name": "chapa/chapa",
    "description": "PHP SDK for Chapa payment gateway",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Chapa\\Chapa\\": "src/",
            "Chapa\\": "src/",
            "Chapa\\Models\\": "src/models",
            "Chapa\\Exceptions\\": "src/exceptions"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "guzzlehttp/guzzle": "^7.0",
        "vlucas/phpdotenv": "^5.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    }
}